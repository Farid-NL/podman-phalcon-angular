PHALCON_VERSION=

# Por defecto: 4200
ANGULAR_PORT=

# Por defecto: 5432
APACHE_PORT=

# Por defecto: 8080
DB_PORT=

DB_USERNAME=admin
DB_PASSWORD=secret
DB_NAME=dinero_electronico

